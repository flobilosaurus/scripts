function rbranch --wraps=git\ checkout\ \(git\ fetch\ --quiet\ --all\ --prune\ \&\&\ git\ branch\ -r\ --sort=-committerdate\ --format=\"\%\(refname:lstrip=3\)\|\%\(committerdate:iso8601\)\|\%\(contents:subject\)\"\ \|\ awk\ \'\{\n\ \ \ \ \ \ \ \ \ \ split\(\$0,a,\"\|\"\)\;\n\ \ \ \ \ \ \ \ \ \ \ \ print\ a\[1\]\ \"\|\"\ a\[2\]\ \"\|\"\ a\[3\]\ \}\'\ \|\ awk\ -F\\\|\ \'!a\[\$1\]++\'\ \|\ column\ -t\ -s\ \'\|\'\ \|\ fzf\ \|\ awk\ -F\'\ \ \'\ \'\{print\ \$1\}\'\) --description alias\ rbranch=git\ checkout\ \(git\ fetch\ --quiet\ --all\ --prune\ \&\&\ git\ branch\ -r\ --sort=-committerdate\ --format=\"\%\(refname:lstrip=3\)\|\%\(committerdate:iso8601\)\|\%\(contents:subject\)\"\ \|\ awk\ \'\{\n\ \ \ \ \ \ \ \ \ \ split\(\$0,a,\"\|\"\)\;\n\ \ \ \ \ \ \ \ \ \ \ \ print\ a\[1\]\ \"\|\"\ a\[2\]\ \"\|\"\ a\[3\]\ \}\'\ \|\ awk\ -F\\\|\ \'!a\[\$1\]++\'\ \|\ column\ -t\ -s\ \'\|\'\ \|\ fzf\ \|\ awk\ -F\'\ \ \'\ \'\{print\ \$1\}\'\)
  git checkout (git fetch --quiet --all --prune && git branch -r --sort=-committerdate --format="%(refname:lstrip=3)|%(committerdate:iso8601)|%(contents:subject)" | awk '{
          split($0,a,"|");
            print a[1] "|" a[2] "|" a[3] }' | awk -F\| '!a[$1]++' | column -t -s '|' | fzf | awk -F'  ' '{print $1}') $argv; 
end
